<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:jdbc="http://www.springframework.org/schema/jdbc"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
     http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
     http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
     http://www.springframework.org/schema/jdbc http://www.springframework.org/schema/jdbc/spring-jdbc-3.0.xsd
     http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.0.xsd
     http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.0.xsd">
  <bean class="com.sinoservices.framework.core.support.ApplicationContextRegister" />
	
	<!-- APP Bean配置开始 -->
	<import resource="classpath*:app/selectCode/*.xml"/>
    <import resource="classpath*:app/applicationContext-core.xml"/>
    <import resource="classpath*:app/applicationContext-aop.xml"/>
    <import resource="classpath*:app/applicationContext-cache.xml"/>
    <import resource="classpath*:app/applicationContext-common.xml"/>
    <import resource="classpath*:app/applicationContext-cxf.xml"/>
    <import resource="classpath*:app/applicationContext-exception.xml"/>
    <import resource="classpath*:app/applicationContext-file.xml"/>
    <import resource="classpath*:app/applicationContext-report.xml"/>
    <import resource="classpath*:app/applicationContext-schedule.xml"/>
    <import resource="classpath*:app/applicationContext-cache-security.xml"/>
    <import resource="classpath*:app/applicationContext-xfire.xml"/>
    <import resource="classpath*:message/*.xml"/>
    <import resource="classpath*:app/applicationContext-ldap.xml"/>

	<!-- APP Bean配置结束 -->
	
    <!-- LMDM Bean配置开始 -->
    <import resource="classpath:lmdm/selectCode/*.xml"/>
    <!-- TANGRAM -->
  	<import resource="classpath*:parent/context-autofacade.xml"/> 
    <!--
    <import resource="classpath:lmdm/applicationContext-quartz.xml"/>
    
    -->
    <import resource="classpath:lmdm/applicationContext-share.xml"/>
	<!-- LMDM Bean配置结束 -->
    <!-- OMS2B Bean配置开始 -->
    <import resource="classpath:newoms/selectCode/*.xml"/>
    <import resource="classpath:newoms/applicationContext-omsShare.xml"/>
    <import resource="classpath:newoms/applicationContext-activemq.xml"/>
    <import resource="classpath:newoms/applicationContext-omsThread.xml"/>
    <import resource="classpath:newoms/applicationContext-rpc.xml"/>
    <import resource="classpath:newoms/applicationContext-facade.xml"/>
    <import resource="classpath:newoms/applicationContext-datasource.xml"/>
    <import resource="classpath:newoms/context-aop.xml"/>
	<import resource="classpath:newoms/applicationContext-sign.xml"/>
    <import resource="classpath*:appsupport/context-aop.xml"/>
	<!-- TMS Bean配置结束 -->
	
	<!-- tangram -->
    <context:component-scan base-package="com.sinoservices.xframework"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<!-- APP -->
    <context:component-scan base-package="com.sinoservices.chart"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.chem"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.common"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.components"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.edi"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.framework"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.ireport"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.message"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.querysetting"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.synergy"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.visibility"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.app"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.devmode"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<!-- LMDM -->				
	<context:component-scan base-package="com.sinoservices.lrms"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<!-- OMS2B -->
	<context:component-scan base-package="com.sinoservices.tes"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.oms"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<context:component-scan base-package="com.sinoservices.edimonitor"
							name-generator="com.sinoservices.framework.core.support.CustomBeanNameGenerator" />
	<bean id="oms2bAuditor" class="com.sinoservices.components.audit.service.impl.AuditLogImpl"> 
 		<property name="table" value="EO_ORDER_AUDIT_LOG"></property>
 		<property name="module" value="oms2b"></property>
 		<property name="desc" value="公司操作日志"></property>
    </bean>
    <bean id="auditLogWork" class="com.sinoservices.components.audit.service.impl.AuditLogImpl"> 
		<property name="table" value="OMS_AUDIT_LOG_WORK"></property>
		<property name="module" value="omsWork"></property>
	</bean>	
</beans>