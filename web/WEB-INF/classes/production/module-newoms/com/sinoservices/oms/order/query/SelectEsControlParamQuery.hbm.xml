<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<sql-query name="SelectEsControlParamQuery">
		<![CDATA[
		
		select ESCP_VALUE from ES_CONTROL_PARAM where escp_code = :escpCode
		   
		]]>
	</sql-query>
</hibernate-mapping>