<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<sql-query name="DeleteEoSerialNoUpdate">
		<![CDATA[
delete from eo_serial_no eosn
 where 1 = 1
   and eosn.eosn_id in (:ids)
		]]>
	</sql-query>
</hibernate-mapping>