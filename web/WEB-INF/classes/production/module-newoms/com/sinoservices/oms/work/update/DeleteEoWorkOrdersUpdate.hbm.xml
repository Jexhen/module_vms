<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<sql-query name="DeleteEoWorkOrdersUpdate">
		<![CDATA[

	DELETE FROM EO_WORK_ORDER EOWO WHERE EOWO.EOWO_ORDER_NO IN (:ids)


		]]>
	</sql-query>
</hibernate-mapping>
